{
  "name": "watchtime-api",
  "version": "0.1.0",
  "description": "A watchtime API route for a bot to store chatter watch time for a stream.",
  "type": "module",
  "main": "./server/server.js",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./server/server.js",
    "dev": "SET \"NODE_ENV=development\"&&npm-run-all server",
    "prod": "SET \"NODE_ENV=production\"&&npm-run-all server",
    "transpile": "babel ./server --out-dir dist",
    "watch:dev": "nodemon",
    "clean": "rimraf ./dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ]
  },
  "keywords": [
    "api",
    "rest",
    "twitch"
  ],
  "author": "saulprl",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.0.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
