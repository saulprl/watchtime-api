{
  "name": "watchtime-api",
  "version": "0.1.0",
  "description": "A watchtime API route for a bot to store chatter watch time for a stream.",
  "main": "./server/index.js",
  "scripts": {
    "start": "node ./server/index.js",
    "build": "npm-run-all clean transpile",
    "transpile": "babel ./server --out-dir dist",
    "clean": "rimraf ./dist-server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "keywords": [
    "api",
    "rest",
    "twitch"
  ],
  "author": "saulprl",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.0.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
