{
  "name": "watchtime-api",
  "version": "0.4.9",
  "description": "A watchtime API route for a bot to store chatter watch time for a stream.",
  "type": "module",
  "main": "./server/server.js",
  "scripts": {
    "server": "node ./server/server.js",
    "start": "node ./server/server.js",
    "dev": "npm run server",
    "prod": "npm run server",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "controllers/*"
    ]
  },
  "keywords": [
    "api",
    "rest",
    "twitch"
  ],
  "author": "saulprl",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0",
    "toad-scheduler": "^2.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
